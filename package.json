    "reason-react": "0.3.2",
    "webpack": "3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexfedoseev/bs-react-intl.git"
  },
  "files": [
    "src",
    "bsconfig.json"
  ],
