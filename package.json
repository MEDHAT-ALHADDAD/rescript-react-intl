{
  "name": "bs-react-intl",
  "version": "0.4.0",
  "description": "BuckleScript bindings to react-intl",
  "author": "Alex Fedoseev <alex.fedoseev@gmail.com>",
  "license": "MIT",
  "main": "src/ReactIntl.bs.js",
  "scripts": {
    "start": "bsb -clean-world -make-world && webpack --watch",
    "build": "bsb -clean-world -make-world && webpack",
    "clean": "bsb -clean-world",
    "refmt": "find -E . -regex '.+\\.rei?' -exec refmt --in-place {} \\;",
    "test": "exit 0",
    "preversion": "yarn run clean && yarn run refmt"
  },
  "peerDependencies": {
    "bs-platform": "^3.0.0",
    "react-intl": "^2.4.0",
    "reason-react": "^0.4.1"
  },
  "devDependencies": {
    "bs-platform": "4.0.5",
    "prop-types": "15.6.2",
    "react": "16.5.1",
    "react-dom": "16.5.1",
    "react-intl": "2.4.0",
    "reason-react": "0.5.3",
    "webpack": "3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexfedoseev/bs-react-intl.git"
  },
  "files": [
    "src",
    "bsconfig.json"
  ],
  "keywords": [
    "react",
    "react-intl",
    "reason",
    "reasonml",
    "ocaml",
    "bucklescript"
  ]
}
